<!DOCTYPE html>
<html>
    <head>
        <title>Campgrounds</title>
    </head>
    <body>
        <h1>All Campgrounds</h1>
        <div>
            <a href="/campgrounds/new">Add Campground</a>
        </div>
        <ul>
            <% for(campground of campgrounds){ %>
                <li><a href="campgrounds/<%= campground.id %>"><%= campground.title %></a></li>
            <% } %>
        </ul>
    </body>
</html>